<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="83dp"
        android:layout_height="76dp"
        android:layout_x="14dp"
        android:layout_y="18dp"
        android:background="@color/colorPrimary"
        app:srcCompat="@android:drawable/ic_dialog_dialer" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="97dp"
        android:layout_height="34dp"
        android:layout_x="103dp"
        android:layout_y="15dp"
        android:text="Retailer Name:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:layout_alignBottom="@+id/imageView9"
        android:layout_toRightOf="@+id/imageView9"
        android:layout_toEndOf="@+id/imageView9" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="159dp"
        android:layout_height="38dp"
        android:layout_x="224dp"
        android:layout_y="12dp"
        android:text="Name"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="158dp"
        android:layout_height="39dp"
        android:layout_x="222dp"
        android:layout_y="57dp"
        android:text="Phone Number"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="101dp"
        android:layout_height="36dp"
        android:layout_x="103dp"
        android:layout_y="56dp"
        android:text="Phone No:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView9"
        android:layout_toEndOf="@+id/imageView9" />



</RelativeLayout>
