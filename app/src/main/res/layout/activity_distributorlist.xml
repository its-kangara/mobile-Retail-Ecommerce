    <TextView
        android:id="@+id/textView15"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_x="107dp"
        android:layout_y="60dp"
        android:text="Phone:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1" />


</AbsoluteLayout>
